{
  "name": "file-service",
  "version": "1.0.0",
  "description": "upload files with form loader",
  "main": "app.js",
  "engines": {
    "node": "8.9.4"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "concurrently \"node app.js\" \"npm run build_client\"",
    "dev": "concurrently -k \"nodemon app.js\" \"npm run dev_client\"",
    "dev_client": "npm run dev -C client",
    "build_client": "npm run build --prefix client"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "multer": "^1.3.0",
    "pug": "^2.0.1"
  }
}
